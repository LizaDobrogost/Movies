@model IEnumerable<string>

@{
    ViewBag.Title = "Roles";
}

<hgroup>
    <h2>Roles</h2>
</hgroup>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Role
        </th>
        <th>
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item }) |
                @Html.ActionLink("Delete", "Delete", new { id = item })
            </td>
        </tr>
    }</table>
